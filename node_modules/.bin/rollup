#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/russell/Documents/code/experiments/dspatch/node_modules/rollup/dist/bin/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules/rollup/dist/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules/rollup/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules:/Users/russell/Documents/code/experiments/node_modules:/Users/russell/Documents/code/node_modules:/Users/russell/Documents/node_modules:/Users/russell/node_modules:/Users/node_modules:/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/russell/Documents/code/experiments/dspatch/node_modules/rollup/dist/bin/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules/rollup/dist/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules/rollup/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules:/Users/russell/Documents/code/experiments/node_modules:/Users/russell/Documents/code/node_modules:/Users/russell/Documents/node_modules:/Users/russell/node_modules:/Users/node_modules:/node_modules:/Users/russell/Documents/code/experiments/dspatch/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../rollup/dist/bin/rollup" "$@"
fi
